package main

import (
	"fmt"
	"strings"
)

func main() {
	data := "apple,banala,orange"
	data2 := "apple.banala.orange"

	part := strings.Split(data, ",")
	part2 := strings.Split(data2, ".")
	fmt.Println(part)
	fmt.Println(part2)

}


outputs:- [apple banala orange]
          [apple banala orange]




package main

import (
	"fmt"
	"strings"
)

func main() {

	str := "one,two,three,three,four"

	count := strings.Count(str, "three")

	fmt.Println("count is:-", count)


	

}
